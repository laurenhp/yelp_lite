<div class="page-header">
  <h1>
    Photosets
    <a href="/photosets/new" class="btn btn-lg btn-success">New Photoset</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Description</th>
        <th>Actions</th>
      </tr>

      <% @photosets.each do |photoset| %>
      <tr>
        <td><%= photoset.description %></td>
        <td>
          <a href="/photosets/<%= photoset.id %>" class="btn btn-primary">Show</a>
          <a href="/photosets/<%= photoset.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_photoset/<%= photoset.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
    <%= paginate @photosets, theme: 'twitter-bootstrap-4' %>
  </div>
</div>
